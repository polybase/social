{
  "name": "spacetime-social",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "load": "ts-node ./schema/load.ts",
    "env": "env-cmd --no-override --file ${ENV_PATH:-.env.development}",
    "deploy": "SKIP_PREFLIGHT_CHECK=true NODE_ENV=production yarn build && yarn deploy:netlify",
    "deploy:netlify": "netlify deploy --dir ./build --prod --functions ./noop",
    "fix": "eslint \"./src/**/*.{ts,tsx}\" webpack.config.js --fix"
  },
  "dependencies": {
    "@1productaweek/react-modal-hooks": "^0.1.14",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "@spacetimexyz/client": "^0.2.0",
    "@spacetimexyz/eth": "^0.1.27",
    "@spacetimexyz/react": "^0.2.0",
    "@spacetimexyz/util": "^0.1.15",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "axios": "^0.27.2",
    "ethereumjs-wallet": "^1.0.2",
    "framer-motion": "^6.2.9",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nanoid": "^4.0.0",
    "posthog-js": "^1.29.0",
    "react": "^18.1.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.1.0",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.34.2",
    "react-icons": "^3.0.0",
    "react-infinite-scroller": "^1.2.6",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.3",
    "use-interval": "^1.4.0",
    "web-vitals": "^0.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": [
        2,
        {
          "skipUndeclared": true
        }
      ],
      "react/jsx-handler-names": "off",
      "quotes": "off",
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "react/display-name": 0,
      "semi": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "arrow-body-style": 0,
      "no-use-before-define": 0,
      "arrow-parens": 0,
      "no-trailing-spaces": "error",
      "@typescript-eslint/quotes": [
        "error",
        "single"
      ],
      "@typescript-eslint/indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/react-helmet": "^6.1.5",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-loadable": "^5.5.6",
    "@types/react-resizable": "^3.0.2",
    "@types/react-table": "^7.7.12",
    "add": "^2.0.6",
    "assert": "^2.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.0",
    "env-cmd": "^10.1.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-textarea-autosize": "^8.3.4",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-node": "^10.9.1",
    "url": "^0.11.0",
    "yarn": "^1.22.19"
  }
}
